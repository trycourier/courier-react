{
  "name": "@trycourier/react-preferences",
  "version": "3.7.0",
  "main": "dist/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "babel": "babel src -d dist --extensions \".ts,.tsx\" --ignore \"src/**/__tests__/**\"",
    "build:watch": "yarn babel --watch",
    "build": "rimraf dist && yarn babel",
    "clean": "rimraf dist",
    "readme": "concat-md --toc --decrease-title-levels --dir-name-as-title docs > README.md",
    "type-check": "tsc --noEmit",
    "types": "tsc --build"
  },
  "peerDependencies": {
    "@trycourier/react-provider": ">=3.X.X",
    "react": ">=17.X.X",
    "react-dom": ">=17.X.X"
  },
  "files": [
    "dist/",
    "typings/"
  ],
  "license": "ISC",
  "dependencies": {
    "@trycourier/react-hooks": "^3.7.0",
    "react-toggle": "^4.1.2",
    "styled-components": "^5.3.6"
  }
}
